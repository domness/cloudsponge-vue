{
  "name": "cloudsponge-vue",
  "version": "1.0.0",
  "description": "Vue component to launch the Cloudsponge address book",
  "license": "MIT",
  "private": false,
  "main": "dist/cloudsponge-vue.ssr.js",
  "browser": "dist/cloudsponge-vue.esm.js",
  "module": "dist/cloudsponge-vue.esm.js",
  "unpkg": "dist/cloudsponge-vue.min.js",
  "types": "cloudsponge-vue.d.ts",
  "files": [
    "dist/*",
    "cloudsponge-vue.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "release": "standard-version"
  },
  "dependencies": {
    "vue": "^3.1.2",
    "vue-template-compiler": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^5.0.0-beta.2",
    "@vue/cli-plugin-typescript": "^5.0.0-beta.2",
    "@vue/cli-service": "^5.0.0-beta.2",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.52.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "vue": "^3.1.2"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:domness/cloudsponge-vue.git"
  }
}
